display2d:false$/*##*/
linel:1000$/*##*/
batch("/Applications/KeTTeX.app/texlive/texmf-dist/scripts/ketcindy/ketlib/maximaL/mnr.max")$/*##*/
lft:((1-M2)^2-4*M2)/((1-M2)^2+4*M2)$/*##*/
rgt:(c^2+a^2-b^2)/(2*c*a)$/*##*/
eq:expand(numer(lft-rgt))$/*##*/
